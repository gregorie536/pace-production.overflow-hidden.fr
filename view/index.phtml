<script>
	const RAYON = 250
	let elem = document.getElementById("points")
	let arr = Array.prototype.slice.call( elem.children )

	let decale = 360 / arr.length

	setTimeout(() => {
		arr.map((e, i) => {
			e.style.top = cosDegrees((decale*(i+1))-decale)+"px";
			e.style.left = sinDegrees((decale*(i+1))-decale)+"px";
			e.classList.add("text")
			
			if(i == 3)
				setPointColor(e)
		})
	}, 1000);

	let selected = 3
	setInterval(() => {
		setPointColor(arr[selected], "white")
		
		selected++
		if(selected >= arr.length)
			selected =  0
		
		setPointColor(arr[selected])

	}, 1000*12); /* 1000ms * 12s */

	function setPointColor(elem, color = "#eeb641"){
		elem.firstElementChild.style.backgroundColor = color;
		
		if(color == "#eeb641"){
			arr[selected].classList.add("active")
		}else{
			arr[selected].classList.remove("active")
		}
	}

	function sinDegrees(angleDegrees) {
		return Math.sin(angleDegrees*Math.PI/180)*RAYON;
	};
	function cosDegrees(angleDegrees) {
		return Math.cos(angleDegrees*Math.PI/180)*RAYON;
	};
</script>

